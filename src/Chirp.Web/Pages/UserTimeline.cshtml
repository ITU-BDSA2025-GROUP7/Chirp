@page "/{author}"
@model Chirp.Razor.Pages.UserTimelineModel
@{
    ViewData["Title"] = "Chirp!";
    Layout = "Shared/_Layout";
}

<div>
    <h2>@Model.Header</h2>
    <partial name="Shared/_sendCheepPartial" model="@Model"/>
    @if (Model.Author != null) {
        <partial name="Shared/_CheepListPartial" model="@Model"/>
    }
</div>
